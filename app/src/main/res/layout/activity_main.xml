<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="493dp"
        android:layout_height="849dp"
        app:srcCompat="@drawable/b59f89bf7eb4d12f188344f01d575c70"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="-47dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="262dp"
        android:layout_height="40dp"
        android:background="#B9F6CA"
        android:text="Password"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="335dp"
        tools:text="Password" />

    <TextView
        android:layout_width="262dp"
        android:layout_height="40dp"
        android:background="#B9F6CA"
        android:text="Usuario"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="229dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="424dp"
        android:layout_height="50dp"
        android:background="#84FFFF"
        android:text="Ingresa lo que se te pude para entrar"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:drawableTint="#F4FF81"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="50dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#448AFF"
        android:text="Entrar"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="437dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FF5252"
        android:text="Cancelar"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="438dp" />

</androidx.constraintlayout.widget.ConstraintLayout>